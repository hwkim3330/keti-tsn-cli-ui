{
  "name": "keti-tsn-cli-ui",
  "version": "1.0.0",
  "description": "TSN Switch Configuration Web UI",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "npm run server",
    "install:all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "cap": "^0.2.1",
    "cbor": "^9.0.0",
    "cbor-x": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "multer": "^1.4.5-lts.1",
    "serialport": "^12.0.0",
    "ws": "^8.19.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
